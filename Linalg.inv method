# linalg.inv method

# EXAMPLE A
import numpy as np
from numpy.linalg import inv

A = np.array([[3.5, 5.7], [2.9, 6.1]])

result = inv(A)

print(f'EXAMPLE A:{result}')

# EXAMPLE B
import numpy as np
from numpy.linalg import inv

A = np.array([[2, 4], [6, 8]])

result = inv(A)

print(f'EXAMPLE B:{result}')

# EXAMPLE C
import numpy as np
from numpy.linalg import inv

A = np.array([[2, 3], [4,5]])

result = inv(A)

print(f'EXAMPLE C:{result}')